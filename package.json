{
  "name": "contact-it",
  "version": "1.0.0",
  "description": "contacts management application",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build-local": "webpack",
    "watch": "webpack-dev-server",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/B-Pratik/contact-it.git"
  },
  "keywords": [
    "contacts"
  ],
  "author": "Pratik Bhurewar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B-Pratik/contact-it/issues"
  },
  "homepage": "https://github.com/B-Pratik/contact-it#readme",
  "browserslist": [
    "last 1 version"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Promise": true
    },
    "extends": [
      "react",
      "prettier",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "antd": "^4.5.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-webpack-plugin": "^4.3.0",
    "lodash-es": "^4.17.15",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^1.2.1",
    "vcf": "^2.1.0",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  }
}
